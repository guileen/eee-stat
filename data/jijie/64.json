    {
      "type": "卦",
      "name": "未濟",
      "down": "坎",
      "up": "離",
      "lines": [
        {
          "type": "卦名",
          "value": "　未濟　坎下離上"
        },
        {
          "type": "句",
          "value": "未濟，亨。小狐汔濟，",
          "children": [
            {
              "type": "释文",
              "text": "狐，徐音胡。"
            },
            {
              "type": "集解",
              "list": [
                {
                  "text": " 鄭康成曰：汔，幾也。",
                  "book": "［釋文］"
                }
              ]
            }
          ]
        },
        {
          "type": "句",
          "value": "濡其尾，无攸利。"
        }
      ]
    },
    {
      "type": "彖",
      "lines": [
        {
          "type": "句",
          "value": "《彖》曰：未濟亨，柔得中也。"
        },
        {
          "type": "句",
          "value": "小狐汔濟，未出中也。濡其尾，无攸利，不續終也。雖不當位，剛柔應也。"
        }
      ]
    },
    {
      "type": "大象",
      "lines": [
        {
          "type": "句",
          "value": "《象》曰：火在水上，未濟。君子以慎，辨物居方。"
        }
      ]
    },
    {
      "type": "爻",
      "index": 1,
      "lines": [
        {
          "type": "句",
          "value": "初六，濡其尾，吝。"
        }
      ]
    },
    {
      "type": "小象",
      "index": 1,
      "lines": [
        {
          "type": "句",
          "value": "《象》曰：濡其尾，亦不知極也。"
        }
      ]
    },
    {
      "type": "爻",
      "index": 2,
      "lines": [
        {
          "type": "句",
          "value": "九二，曳其輪，貞吉。"
        }
      ]
    },
    {
      "type": "小象",
      "index": 2,
      "lines": [
        {
          "type": "句",
          "value": "《象》曰：九二貞吉，中以行正也。"
        }
      ]
    },
    {
      "type": "爻",
      "index": 3,
      "lines": [
        {
          "type": "句",
          "value": "六三，未濟，征凶，利涉大川。"
        }
      ]
    },
    {
      "type": "小象",
      "index": 3,
      "lines": [
        {
          "type": "句",
          "value": "《象》曰：未濟征凶，位不當也。"
        }
      ]
    },
    {
      "type": "爻",
      "index": 4,
      "lines": [
        {
          "type": "句",
          "value": "九四，貞吉，悔亡。震用伐鬼方，三年有賞于大國。"
        }
      ]
    },
    {
      "type": "小象",
      "index": 4,
      "lines": [
        {
          "type": "句",
          "value": "《象》曰：貞吉悔亡，志行也。"
        }
      ]
    },
    {
      "type": "爻",
      "index": 5,
      "lines": [
        {
          "type": "句",
          "value": "六五，貞吉，无悔。君子之光，有孚，吉。"
        }
      ]
    },
    {
      "type": "小象",
      "index": 5,
      "lines": [
        {
          "type": "句",
          "value": "《象》曰：君子之光，其暉吉也。",
          "children": [
            {
              "type": "释文",
              "text": "暉字又作輝。"
            }
          ]
        }
      ]
    },
    {
      "type": "爻",
      "index": 6,
      "lines": [
        {
          "type": "句",
          "value": "上九，有孚于飲酒，无咎。濡其首，有孚失是。"
        }
      ]
    },
    {
      "type": "小象",
      "index": 6,
      "lines": [
        {
          "type": "句",
          "value": "《象》曰：飲酒濡首，亦不知節也。"
        }
      ]
    }
  ]
}