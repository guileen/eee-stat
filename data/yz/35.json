      "lines": [
        {
          "type": "卦名",
          "value": "35. ䷢　 晉卦 　坤下離上"
        },
        {
          "type": "句",
          "value": "晉，康侯用錫馬蕃庶，晝日三接。"
        }
      ]
    },
    {
      "type": "彖",
      "lines": [
        {
          "type": "句",
          "value": "彖曰：晉，進也。明出地上，順而麗乎大明，柔進而上行，是以康侯用錫馬蕃庶，晝日三接也。"
        }
      ]
    },
    {
      "type": "大象",
      "lines": [
        {
          "type": "句",
          "value": "象曰：明出地上，晉。君子以自昭明德。"
        }
      ]
    },
    {
      "type": "爻",
      "index": 1,
      "lines": [
        {
          "type": "句",
          "value": "初六，晉如摧如，貞吉。罔孚，裕无咎。"
        }
      ]
    },
    {
      "type": "小象",
      "index": 1,
      "lines": [
        {
          "type": "句",
          "value": "象曰：晉如摧如，獨行正也；裕无咎，未受命也。"
        }
      ]
    },
    {
      "type": "爻",
      "index": 2,
      "lines": [
        {
          "type": "句",
          "value": "六二，晉如愁如，貞吉。受茲介福，于其王母。"
        }
      ]
    },
    {
      "type": "小象",
      "index": 2,
      "lines": [
        {
          "type": "句",
          "value": "象曰：受茲介福，以中正也。"
        }
      ]
    },
    {
      "type": "爻",
      "index": 3,
      "lines": [
        {
          "type": "句",
          "value": "六三，眾允，悔亡。"
        }
      ]
    },
    {
      "type": "小象",
      "index": 3,
      "lines": [
        {
          "type": "句",
          "value": "象曰：眾允之，志上行也。"
        }
      ]
    },
    {
      "type": "爻",
      "index": 4,
      "lines": [
        {
          "type": "句",
          "value": "九四，晉如鼫鼠，貞厲。"
        }
      ]
    },
    {
      "type": "小象",
      "index": 4,
      "lines": [
        {
          "type": "句",
          "value": "象曰：鼫鼠貞厲，位不當也。"
        }
      ]
    },
    {
      "type": "爻",
      "index": 5,
      "lines": [
        {
          "type": "句",
          "value": "六五，悔亡，失得勿恤。往吉，无不利。"
        }
      ]
    },
    {
      "type": "小象",
      "index": 5,
      "lines": [
        {
          "type": "句",
          "value": "象曰：失得勿恤，往有慶也。"
        }
      ]
    },
    {
      "type": "爻",
      "index": 6,
      "lines": [
        {
          "type": "句",
          "value": "上九，晉其角，維用伐邑。厲吉无咎，貞吝。"
        }
      ]
    },
    {
      "type": "小象",
      "index": 6,
      "lines": [
        {
          "type": "句",
          "value": "象曰：維用伐邑，道未光也。"
        }
      ]
    }
  ]
}